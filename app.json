{
  "expo": {
    "name": "ChocoBills",
    "slug": "ChocoBills",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/subs_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devrickk.ChocoBills",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/subs_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devrickk.ChocoBills"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/subs_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/subs_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c050fc73-cd53-41ea-86b8-d236cba7e5b6"
      }
    }
  }
}
